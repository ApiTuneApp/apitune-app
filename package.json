{
  "name": "apitune-app",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "ca:status": "ts-node --project tsconfig.bin.json src/main/bin/proxyhub-ca.ts status",
    "ca:gen": "ts-node --project tsconfig.bin.json src/main/bin/proxyhub-ca.ts genRootCa",
    "ca:trust": "ts-node --project tsconfig.bin.json src/main/bin/proxyhub-ca.ts trust"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@glideapps/glide-data-grid": "^6.0.3",
    "@mui/icons-material": "^5.15.6",
    "@mui/material": "^5.15.6",
    "async": "^3.2.5",
    "cacheable-lookup": "^6.1.0",
    "electron-updater": "^6.1.7",
    "koa": "^2.15.0",
    "lodash": "^4.17.21",
    "marked": "^12.0.0",
    "mkdirp": "^3.0.1",
    "node-easy-cert": "^1.3.3",
    "node-forge": "^1.3.1",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.22.0",
    "replacestream": "^4.0.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/koa": "^2.14.0",
    "@types/node": "^18.19.9",
    "@types/node-easy-cert": "^1.3.4",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/replacestream": "^4.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "less": "^4.2.0",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  }
}
